{
  "name": "@mathieustan/vue-intercom",
  "description": "A wrapper for Intercom",
  "version": "0.0.7",
  "author": "Mathieu Stanowski <mathieu.stan@gmail.com>",
  "keywords": [
    "vue",
    "vuejs",
    "javascript",
    "vue-component",
    "intercom"
  ],
  "license": "MIT",
  "files": [
    "dist/*.js"
  ],
  "main": "dist/vue-intercom.umd.js",
  "module": "dist/vue-intercom.esm.js",
  "unpkg": "dist/vue-intercom.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathieustan/vue-intercom.git"
  },
  "bugs": {
    "url": "https://github.com/mathieustan/vue-intercom/issues"
  },
  "homepage": "https://vue-intercom.netlify.com/",
  "scripts": {
    "commit": "git cz",
    "dev": "npm run build:es -- --watch",
    "build": "npm run build:browser && npm run build:es && npm run build:umd",
    "build:doc": "cd doc-src && npm install && npm run build",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "send:coverage": "cat ./tests/coverage/lcov.info | codacy-coverage",
    "lint": "vue-cli-service lint --ext .js,.vue src tests/unit/* build/*",
    "lint:fix": "npm run lint --fix",
    "test:client": "NODE_ENV=test vue-cli-service test:unit",
    "prepublishOnly": "npm run lint:fix && npm run test:client && npm run build"
  },
  "dependencies": {},
  "peerDependencies": {
    "core-js": "^3.14.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-plugin-unit-jest": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "codacy-coverage": "^3.4.0",
    "commitizen": "^4.1.2",
    "core-js": "^3.14.0",
    "cz-customizable": "^6.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "handlebars": "4.5.0",
    "husky": "^4.2.5",
    "rollup": "^1.14.6",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-visualizer": "^4.0.4",
    "rollup-plugin-vue": "4.2.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "resolutions": {
    "handlebars": "4.5.0"
  },
  "directories": {
    "test": "tests"
  }
}
